<!DOCTYPE html>
<html ng-app="app">
<head>
  <title>TimeKiller</title>

  <%= favicon_link_tag 'favicon.ico' %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="sidebar">
  <%= render :partial=>'salesforce/index', locals: { sobjects: @salesforce.sobjects } %>
</div>

<div id="main_view">
	<%= yield %>
</div>

<div ng-controller="SalesforceCtrl">
	{{sobjects}}
</div>

</body>
</html>
